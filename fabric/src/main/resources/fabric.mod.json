  },
  "icon": "assets/texturelocaleredirector/icon.png",
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.wulian.texturelocaleredirector.fabric.TextureLocaleRedirectorClientFabric"
    ]
  },
  "mixins": [
    "texturelocaleredirector.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20.5"
  }
}